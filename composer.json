{
    "name": "varenykyphp/reviews",
    "description": "VarenykyPHP reviews package",
    "license": "proprietary",
    "authors": [
        {
            "name": "Deniz Tezcan",
            "homepage": "https://www.nopulp.nl/"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "VarenykyReviews\\": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VarenykyReviews\\Support\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
